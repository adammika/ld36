pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
ncol=8 -- number of columns
nrow=8 -- number of rows
mfov=8 -- max fov
level_map={}
player={}

start=192
wall =254
floor=255


function _init()
 load_level(1)
end

function load_level(l)
 if l==1 then
  player.fov=1
  load_level_map(0,0,8,8)
 end
end

function load_level_map(x,y,w,h)
 level_map={}

 for row=y,h do
 level_map[row]={}
  for col=x,w do
   v=mget(col-1,row-1)
   level_map[row][col]=v

   if v==start then
    player.x=col
    player.y=row
   end
  end
 end
end

-- update

function _update()
 u=mget(player.x-1,player.y-2)
 d=mget(player.x-1,player.y)
 l=mget(player.x-2,player.y-1)
 r=mget(player.x,player.y-1)

 if btnp(0) and l==floor then player.x-=1
 elseif btnp(1) and r==floor then player.x+=1
 elseif btnp(2) and u==floor then player.y-=1
 elseif btnp(3) and d==floor then player.y+=1 end

 if btnp(4) and player.fov>1    then player.fov-=1 end
 if btnp(5) and player.fov<mfov then player.fov+=1 end

end

-- drawing

function _draw()
 cls()
 print("x="..player.x)
 print("y="..player.y)
 print("fov="..player.fov)
	draw_level_map()
end

function draw_level_map()
 draw_map()
end

function draw_map()
 for row=1,nrow do
  for col=1,ncol do

   if should_draw(col,row) then

    v=level_map[row][col]
    if v==wall then
     draw_tile(col,row-1,3,4,19,20,-8,-3)
    elseif v==floor or v==start then
     draw_tile(col,row,1,2,17,18,0,0)
    end

    if col==player.x and row==player.y then
     draw_tile(player.x,player.y,5,6,21,22,0,-6)
    end

   end
  end
 end
end

function should_draw(x,y)
 return abs(x-player.x)+abs(y-player.y)<=player.fov
end

function draw_tile(col,row,s1,s2,s3,s4,xoff,yoff)
 palt(0,true)
 x=col+nrow-row
 y=row+ncol+col
 x1=(x-1)*8+xoff
 x2=(x  )*8+xoff
 y1=(y)*4-8+yoff
 y2=(y)*4+yoff
 spr(s1,x1,y1)
 spr(s2,x2,y1)
 spr(s3,x1,y2)
 spr(s4,x2,y2)
end

